{
  "name": "lesca-node-bunnycdn",
  "private": false,
  "description": "use bunny CDN api by node",
  "version": "0.0.21",
  "type": "module",
  "keywords": [
    "bunnycdn",
    "lesca"
  ],
  "files": [
    "lib"
  ],
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"npx tsx src/api.ts\" \"NODE_ENV=DEV vite --host\"",
    "build": "NODE_ENV=production tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "rm": "rm -rf node_modules/ dist/ package-lock.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.9",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "express": "^5.2.1",
    "globals": "^17.0.0",
    "multer": "^2.0.2",
    "path": "^0.12.7",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "sharp": "^0.34.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.25.0"
  }
}
